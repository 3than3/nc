import math

fun print_i(a: int) @extern
fun print_c(a: char) @extern
fun print_b(a: bool) @extern
fun print_s(a: string) @extern

fun print(x: u32) @export
{
    print_i(x)
}

fun print(x: string) @export
{
    print_s(x)
}

fun print(x: bool) @export
{
    print_b(x)
}

fun print(x: char) @export
{
    print_c(x)
}
