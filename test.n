@extern
fun print_int(a: int) -> void
@extern
fun print_char(a: char) -> void
@extern
fun print_bool(a: bool) -> void

fun multiply(a: int, b: int) -> int
{
    return a * b
}

fun main() -> int
{
    var a: int = 2
    var b: int = 4

    var c: int = multiply(a, b)

    print_int(c)

    var x: bool = true
    var y = 'h'

    y = 2

    print_bool(x)
    print_char(y)

    return c
}