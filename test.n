@extern fun print_int(a: int)
@extern fun print_char(a: char)
@extern fun print_bool(a: bool)
@extern fun print_string(a: string)

fun main() -> int
{
    val a = 5

    if (a > 5)
    {
        print_string("A is greater than 5")
    }
    else if (a == 5)
    {
        print_string("A is equal to 5")
    }
    else
    {
        print_string("A is not greater than 5")
    }

    return 0
}