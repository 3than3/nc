@extern fun print_i(a: int)
@extern fun print_c(a: char)
@extern fun print_b(a: bool)
@extern fun print_s(a: string)

fun print(x: int)
{
    print_i(x)
}

fun print(x: string)
{
    print_s(x)
}

fun print(x: bool)
{
    print_b(x)
}

fun print(x: char)
{
    print_c(x)
}

fun say_hello(name: string)
{
    print("Hello there, ")
    print(name)
}

fun say_hello(name: string)
{
    print("Hi there, ")
    print(name)
}

fun say_hello(initial: char)
{
    print("Hello there, ")
    print(initial)
}

fun main() -> int
{
    print(5)
    print(true)
    print('H')

    say_hello("John")
    say_hello("H")

    return 0
}